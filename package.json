{
  "name": "noodown-fastify",
  "version": "1.0.0",
  "description": "Observability middleware for fastify that sends HTTP request logs to the noodown.com service.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "types": "./dist/cjs/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fastify",
    "middleware",
    "observability",
    "logging",
    "monitoring",
    "noodown",
    "http",
    "request-logging"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dhmeson/noodown-fastify"
  },
  "bugs": {
    "url": "https://github.com/Dhmeson/noodown-fastify/issues"
  },
  "homepage": "https://noodown.com",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  },
 
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  }
}
